<?xml version="1.0" encoding="utf-8"?>
<com.shanlinjinrong.oa.views.SquareLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:orientation="horizontal">

    <com.shanlinjinrong.oa.views.SquareLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center">

        <ImageView
            android:id="@+id/image"
            android:layout_width="@dimen/common_image_width"
            android:layout_height="@dimen/common_image_width"
            android:src="@drawable/icon_launch_approval_supplies" />

        <TextView
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/common_top_space"
            android:text="@string/approval_office_supplies"
            android:textColor="@color/black_333333"
            android:textSize="@dimen/common_text_size" />
    </com.shanlinjinrong.oa.views.SquareLinearLayout>

    <TextView
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@color/gray_E5E5E5"/>
</com.shanlinjinrong.oa.views.SquareLinearLayout>
